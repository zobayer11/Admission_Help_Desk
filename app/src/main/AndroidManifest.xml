<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trodev.admissionhelpdesk">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdmissionHelpDesk"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".DeveloperActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".university.JahangirActivity"
            android:exported="false"
            android:parentActivityName=".UniversityActivity" />
        <activity
            android:name=".university.CuActivity"
            android:exported="false"
            android:parentActivityName=".UniversityActivity" />
        <activity
            android:name=".university.RuActivity"
            android:exported="false"
            android:parentActivityName=".UniversityActivity" />
        <activity
            android:name=".university.DuActivity"
            android:exported="false"
            android:parentActivityName=".UniversityActivity" />
        <activity
            android:name=".university.NationalActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".university.AgriActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RequestDeveloperActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".university.GucchoActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".university.RckActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".university.BuetActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UniversityActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>